{
  "name": "mingly",
  "version": "0.2.1",
  "description": "Mingly - Mingle with all AI minds in one place",
  "main": "dist/main/index.js",
  "author": {
    "name": "Mingly Contributors",
    "email": "welcome@digital-opua.ch"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:vite\" \"npm run dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "wait-on http://localhost:5173 && electron .",
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "tsc -p tsconfig.main.json",
    "build:renderer": "vite build",
    "start": "electron .",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "typecheck": "tsc --noEmit && tsc -p tsconfig.main.json --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "build:server": "tsc -p tsconfig.server.json",
    "start:server": "node dist-server/server/index.js"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@google/generative-ai": "^0.24.1",
    "@qdrant/js-client-rest": "^1.16.2",
    "@xenova/transformers": "^2.17.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "electron-store": "^11.0.2",
    "electron-updater": "^6.7.3",
    "lucide-react": "^0.563.0",
    "nanoid": "^5.1.6",
    "ollama": "^0.6.3",
    "openai": "^6.16.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^16.1.0",
    "react-virtuoso": "^4.18.1",
    "remark-gfm": "^4.0.1",
    "sql.js": "^1.13.0",
    "tailwind-merge": "^3.4.0",
    "uuid": "^9.0.1",
    "ws": "^8.19.0",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^4.0.18",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "electron": "^27.3.11",
    "electron-builder": "^24.9.1",
    "jsdom": "^28.0.0",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitest": "^4.0.18",
    "wait-on": "^7.2.0"
  }
}
